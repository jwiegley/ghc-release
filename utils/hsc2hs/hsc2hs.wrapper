#!/bin/sh

tflag="--template=$datadir/template-hsc.h"
for arg do
    case "$arg" in
        -c*)          HSC2HS_EXTRA=;;
        --cc=*)       HSC2HS_EXTRA=;;
        -t*)          tflag=;;
        --template=*) tflag=;;
        --)           break;;
    esac
done

exec $executablename $tflag $HSC2HS_EXTRA ${1+"$@"}
