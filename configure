    if test -f VERSION_DATE; then
        PACKAGE_VERSION=${PACKAGE_VERSION}.`cat VERSION_DATE`
        { echo "$as_me:$LINENO: result: given $PACKAGE_VERSION" >&5
echo "${ECHO_T}given $PACKAGE_VERSION" >&6; }
    elif test -d .git; then
                ver_date=`git log -n 1 --date=short --pretty=format:%ci | sed "s/^.*\([0-9][0-9][0-9][0-9]\)-\([0-9][0-9]\)-\([0-9][0-9]\).*$/\1\2\3/"`
        if echo $ver_date | grep '^[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]$' 2>&1 >/dev/null; then true; else
                        { { echo "$as_me:$LINENO: error: failed to detect version date: check that git is in your path" >&5
echo "$as_me: error: failed to detect version date: check that git is in your path" >&2;}
   { (exit 1); exit 1; }; }
        fi
        PACKAGE_VERSION=${PACKAGE_VERSION}.$ver_date
        { echo "$as_me:$LINENO: result: inferred $PACKAGE_VERSION" >&5
echo "${ECHO_T}inferred $PACKAGE_VERSION" >&6; }
    elif test -d _darcs; then
        # TODO: Remove this branch after conversion to Git
                ver_date=`darcs changes --quiet --no-summary --xml | head -500 | grep 'date=' | sed "s/^.*date='\([0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]\).*$/\1/g" | sort -n | tail -1`
        if echo $ver_date | grep '^[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]$' 2>&1 >/dev/null; then true; else
                        { { echo "$as_me:$LINENO: error: failed to detect version date: check that darcs is in your path" >&5
echo "$as_me: error: failed to detect version date: check that darcs is in your path" >&2;}
   { (exit 1); exit 1; }; }
        fi
        PACKAGE_VERSION=${PACKAGE_VERSION}.$ver_date
        { echo "$as_me:$LINENO: result: inferred $PACKAGE_VERSION" >&5
echo "${ECHO_T}inferred $PACKAGE_VERSION" >&6; }
    elif test -f VERSION; then
        PACKAGE_VERSION=`cat VERSION`
        { echo "$as_me:$LINENO: result: given $PACKAGE_VERSION" >&5
echo "${ECHO_T}given $PACKAGE_VERSION" >&6; }
    else
        { echo "$as_me:$LINENO: WARNING: cannot determine snapshot version: no .git or _darcs directory and no VERSION file" >&5
echo "$as_me: WARNING: cannot determine snapshot version: no .git or _darcs directory and no VERSION file" >&2;}
    fi
fi

